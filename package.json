{
  "name": "comment-bot-of-bilibili",
  "displayName": "Comment bot of bilibili",
  "version": "0.0.1",
  "description": "comment bot",
  "author": "Asuhe",
  "type": "module",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "compilerOptions": {
    "paths": {
      "~*": [
        "./src/*"
      ]
    },
    "baseUrl": "."
  },
  "dependencies": {
    "plasmo": "0.67.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.210",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "prettier": "2.8.3",
    "typescript": "4.9.4",
    "@plasmohq/prettier-plugin-sort-imports": "3.6.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "permissions": [
      "cookies",
      "https://*.bilibili.com/*"
    ]
  }
}